class Solution:
    def generate(self, numRows: int) -> List[List[int]]:
        def generateRow(row):
            ans = 1
            result = [ans]
            for col in range(1,row):
                ans = ans*(row-col)
                ans = ans // col
                result.append(ans)
            return result
        finalResult = []
        for row in range(1,numRows+1):
            finalResult.append(generateRow(row))
        return finalResult
