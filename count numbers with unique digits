class Solution:
    def countNumbersWithUniqueDigits(self, n: int) -> int:
        if n ==0:
            return 1
        total = 10
        for i in range(2,n+1):
            count = 9
            available = 9
            for j in range(1,i):
                count *= available
                available -= 1
            total += count
        return total
