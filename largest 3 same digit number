class Solution:
    def largestGoodInteger(self, num: str) -> str:
        max_good = ""
        count =1
        for i in range(1,len(num)):
            if num[i] == num[i-1]:
                count += 1
                if count ==3:
                    goodNum = num[i]*3
                    if goodNum > max_good:
                        max_good = goodNum
            else:
                count = 1
        return max_good
